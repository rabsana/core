{
    "name": "rabsana/core",
    "description": "The Rabsana Core",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Sajad Shouli",
        "email": "sajadshouli0@gmail.com"
    }],
    "require-dev": {
        "orchestra/testbench": "^6.20"
    },
    "autoload": {
